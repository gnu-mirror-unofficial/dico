AT_SETUP([exact (all chars)])
AT_KEYWORDS([exact match])
DICTORG_TEST([
database {
	name eng-num;
        handler "dictorg database=eng-num_allchars";
}],
[match eng-num exact "twentyfive"
match eng-num exact "twenty-five"],
[552 No match
152 1 matches found: list follows
eng-num "twenty-five"
.
250
])
AT_CLEANUP
