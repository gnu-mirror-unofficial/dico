AT_SETUP([prefix])
AT_KEYWORDS([prefix match])
DICTORG_TEST([
database {
	name eng-num;
        handler "dictorg database=eng-num";
}],
[match eng-num prefix "twenty"],
[152 10 matches found: list follows
eng-num "twenty"
eng-num "twenty-eight"
eng-num "twenty-five"
eng-num "twenty-four"
eng-num "twenty-nine"
eng-num "twenty-one"
eng-num "twenty-seven"
eng-num "twenty-six"
eng-num "twenty-three"
eng-num "twenty-two"
.
250
])
AT_CLEANUP
